{
  "name": "thunder_api",
  "version": "6.0.0",
  "description": "SplatNet3から取得したJSONをパースしてSplatNet2形式などの扱いやすいフォーマットに変換するAPIです",
  "author": "@tkgstrator",
  "private": true,
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "scripts": {
    "deploy": "wrangler deploy --env dev",
    "dev": "wrangler dev --test-scheduled",
    "generate": "prisma generate --no-engine",
    "migrate": "prisma migrate dev",
    "prepare": "husky",
    "reset": "migrate reset",
    "start": "bun dist/index.js"
  },
  "dependencies": {
    "@prisma/client": "^5.20.0",
    "@prisma/extension-accelerate": "^1.2.1",
    "@scalar/hono-api-reference": "^0.5.152",
    "dayjs": "^1.11.13",
    "hono": "^4.6.3",
    "lodash": "^4.17.21",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.3",
    "@cloudflare/workers-types": "^4.20241004.0",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@commitlint/cz-commitlint": "^19.5.0",
    "@hono/zod-openapi": "^0.16.4",
    "@types/bun": "^1.1.10",
    "@types/deep-diff": "^1.0.5",
    "@types/lodash": "^4.17.10",
    "@types/node": "^22.7.5",
    "@types/uuid": "^10.0.0",
    "commitizen": "^4.3.1",
    "commitlint": "^19.5.0",
    "deep-diff": "^1.0.2",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "prisma": "^5.20.0",
    "rimraf": "^6.0.1",
    "wrangler": "3.80.1"
  },
  "module": "index.ts",
  "type": "module",
  "peerDependencies": {
    "typescript": "^5.6.2"
  }
}
