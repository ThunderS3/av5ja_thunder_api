    "@commitlint/config-conventional": "^19.5.0",
    "@commitlint/cz-commitlint": "^19.5.0",
    "@hono/zod-openapi": "^0.16.2",
    "@types/bun": "^1.1.10",
    "@types/lodash": "^4.17.9",
    "@types/node": "^22.7.4",
    "@types/uuid": "^10.0.0",
    "commitizen": "^4.3.1",
    "commitlint": "^19.5.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
